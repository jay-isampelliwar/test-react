# Agora App ID (used in video/voice calls)
VITE_AGORA_APP_ID=your_agora_app_id_here

# =============================================================================
# AGORA CHAT CONFIGURATION
# =============================================================================

# Agora Chat Host
VITE_AGORA_CHAT_HOST=a61.chat.agora.io

# Agora Chat Organization Name
VITE_AGORA_CHAT_ORG_NAME=your_org_name_here

# Agora Chat App Name
VITE_AGORA_CHAT_APP_NAME=your_app_name_here

# Agora Chat Admin Token (for user registration and status checks)
VITE_AGORA_CHAT_ADMIN_TOKEN=your_admin_token_here

# =============================================================================
# APPLICATION DEFAULTS
# =============================================================================

# Default Token for Video/Voice Calls (fallback)
VITE_DEFAULT_CALL_TOKEN=your_default_call_token_here
